{
    "name": "@milesight/shared",
    "private": true,
    "version": "1.0.0",
    "license": "MIT",
    "description": "Common code for IOT apps.",
    "scripts": {
        "lint": "eslint ./src --cache --cache-location=./node_modules/.cache/.eslintcache",
        "lint:fix": "eslint ./src --fix --quiet --cache --cache-location=./node_modules/.cache/.eslintcache",
        "stylelint": "stylelint ./src/**/*.{css,less} --cache --cache-location=./node_modules/.cache/.stylelintcache",
        "stylelint:fix": "stylelint ./src/**/*.{css,less} --fix --cache --cache-location=./node_modules/.cache/.stylelintcache",
        "ts-check": "tsc --noEmit"
    },
    "dependencies": {
        "@milesight/locales": "workspace:*",
        "@mui/material": "^6.0.2",
        "ahooks": "^3.8.1",
        "axios": "^1.7.7",
        "classnames": "^2.5.1",
        "css-vars-ponyfill": "^2.4.9",
        "immer": "^10.1.1",
        "lodash-es": "^4.17.21",
        "moment": "^2.30.1",
        "moment-duration-format": "^2.3.2",
        "moment-timezone": "^0.5.45",
        "qs": "^6.13.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-intl-universal": "^2.11.1",
        "react-router": "^6.26.1",
        "react-router-dom": "^6.26.1",
        "validator": "^13.12.0",
        "zustand": "^4.5.5"
    },
    "devDependencies": {
        "@milesight/spec": "workspace:*",
        "@types/lodash-es": "^4.17.12",
        "@types/qs": "^6.9.15",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/validator": "^13.12.1",
        "vite": "^5.4.3"
    }
}
