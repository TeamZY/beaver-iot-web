{
    "type": "pieChart",
    "name": "Pie Chart",
    "class": "data_chart",
    "icon": "./icon.png",
    "minCol": 6,
    "minRow": 6,
    "maxCol": 12,
    "maxRow": 12,
    "configProps": [
        {
            "style": "width: 100%",
            "components": [
                {
                    "type": "entitySelect",
                    "title": "Entity",
                    "key": "entity",
                    "style": "width: 100%",
                    "getDataUrl": "",
                    "componentProps": {
                        "size": "small",
                        "entityType": "PROPERTY",
                        "entityValueTypes": ["BOOL", "ENUM"],
                        "accessMods": ["R", "RW"]
                    }
                }
            ]
        },
        {
            "components": [
                {
                    "type": "input",
                    "title": "Title",
                    "key": "title",
                    "style": "width: 100%",
                    "defaultValue": "",
                    "componentProps": {
                        "size": "small",
                        "inputProps": {
                            "maxLength": 15
                        }
                    }
                }
            ]
        },
        {
            "style": "width: 100%;",
            "components": [
                {
                    "type": "ChartTimeSelect",
                    "title": "Time",
                    "key": "time",
                    "style": "width: 100%;",
                    "componentProps": {
                        "size": "small"
                    },
                    "defaultValue": 86400000
                }
            ]
        },
        {
            "style": "width: 100%;",
            "components": [
                {
                    "type": "chartMetricsSelect",
                    "title": "metrics",
                    "key": "metrics",
                    "style": "width: 100%;",
                    "componentProps": {
                        "size": "small"
                    },
                    "defaultValue": "COUNT",
                    "filters": ["LAST","MIN","MAX","AVG","SUM"]
                }
            ]
        }
    ],
    "view": []
}
